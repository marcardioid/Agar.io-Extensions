{
    "manifest_version": 2,
    "name": "Agar.io Server Select",
    "version": "1.0",
    "description": "This extension allows you to input and copy a server ip on agar.io so you can easily play with your friends.",
    "author": "marcardioid",
    "content_scripts":
    [
        {
            "matches": ["http://agar.io/"],
            "js": ["jquery-2.1.4.min.js", "agario-serverselect.js"]
        }
    ],
    "permissions": [
        "http://m.agar.io/"
    ],
  
    "icons":
    {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}